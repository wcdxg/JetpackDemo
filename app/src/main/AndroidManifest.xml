<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yuaihen.jetpack">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.yuaihen.jetpack.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.yuaihen.jetpack.score.ScoreActivity" />
        <activity android:name="com.yuaihen.jetpack.savestate.SaveStateActivity" />
        <activity android:name="com.yuaihen.jetpack.lifecycle.LifeCycleActivity" />
        <activity android:name="com.yuaihen.jetpack.navigation.NavigationActivity" />
        <activity
            android:name="com.yuaihen.jetpack.room.RoomActivity"
            android:windowSoftInputMode="adjustNothing" />
        <activity android:name=".bottomNavigationView.BottomNavigationActivity" />
    </application>

</manifest>